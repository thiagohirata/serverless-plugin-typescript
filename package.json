{
  "name": "@kingdarboja/serverless-plugin-typescript",
  "version": "1.4.0",
  "author": {
    "name": "Prisma Labs",
    "url": "https://www.prisma.io/"
  },
  "maintainers": [
    {
      "name": "KingDarBoja",
      "url": "https://github.com/KingDarBoja/"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:check": "tsc",
    "format": "prettier --loglevel error --write \"{src,tests}/**/*.ts\"",
    "test": "jest",
    "lint": "eslint --ext ts .",
    "prepublishOnly": "npm run format && npm run lint && npm test",
    "preversion": "npm run format && npm run lint && npm test",
    "version": " git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KingDarBoja/serverless-plugin-typescript.git"
  },
  "keywords": [
    "serverless",
    "serverless plugin",
    "typescript",
    "aws lambda"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.1",
    "@types/jest": "26.0.5",
    "@types/lodash": "4.14.157",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.1.0",
    "mock-fs": "4.12.0",
    "prettier": "^2.0.5",
    "ts-jest": "26.1.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "lodash": "^4.17.19"
  }
}
